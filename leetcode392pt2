/**
 * @param {string} s
 * @param {string} t
 * @return {boolean}
 */
var isSubsequence = function(s, t) {
    const t_length = t.length;
    let subsequence = 0;
    for (let i = 0; i < t_length; i++) {
        if (s[subsequence] === t[i]) {
        // ! if it is matching, increment subsequence
        subsequence++;
    }
  }
  return subsequence === s.length 
};

isSubsequence("acb","ahbgdc");